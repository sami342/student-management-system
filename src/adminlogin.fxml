<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="469.0" prefWidth="666.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AdminloginController">
   <children>
      <AnchorPane prefHeight="469.0" prefWidth="355.0" styleClass="navbar" stylesheets="@dashboardDesign.css">
         <children>
            <Label fx:id="m" alignment="BOTTOM_CENTER" contentDisplay="TOP" layoutX="-5.0" prefHeight="375.0" prefWidth="320.0" text="Admin">
               <font>
                  <Font name="Bookman Old Style" size="96.0" />
               </font>
               <graphic>
                  <FontAwesomeIcon fx:id="b2" glyphName="BOOK" />
               </graphic>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="353.0" prefHeight="469.0" prefWidth="376.0">
         <children>
            <Label layoutX="105.0" layoutY="133.0" prefHeight="46.0" prefWidth="137.0" text="Sign In" textFill="#662d60">
               <font>
                  <Font name="Berlin Sans FB" size="41.0" />
               </font>
            </Label>
            <FontAwesomeIcon fill="#662d60" glyphName="USERS" layoutX="121.0" layoutY="119.0" size="8em" />
            <TextField fx:id="username" layoutX="63.0" layoutY="225.0" prefHeight="28.0" prefWidth="220.0" promptText="User name" styleClass="textfield-design" stylesheets="@design.css">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <PasswordField fx:id="password" layoutX="63.0" layoutY="268.0" prefHeight="28.0" prefWidth="220.0" promptText="Password" styleClass="textfield-design" stylesheets="@design.css">
               <font>
                  <Font size="15.0" />
               </font>
            </PasswordField>
            <Button fx:id="login_btn" layoutX="147.0" layoutY="331.0" mnemonicParsing="false" onAction="#login" prefHeight="46.0" prefWidth="88.0" styleClass="button_design" stylesheets="@design.css" text="LogIn" textFill="#f2ebeb">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="ex" layoutX="323.0" layoutY="-2.0" mnemonicParsing="false" onAction="#Exit" prefHeight="24.0" prefWidth="56.0" styleClass="exit" stylesheets="@design.css">
               <graphic>
                  <FontAwesomeIcon fill="#f2e8e8" glyphName="CLOSE" />
               </graphic>
            </Button>
            <Button fx:id="back" layoutX="323.0" layoutY="443.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="56.0" text="Back" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
